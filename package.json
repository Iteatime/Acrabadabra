{
  "name": "acrabadabra",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ENV=prod node scripts/set-env.js && ng build --prod",
    "test": "ENV=dev node scripts/set-env.js && ng test",
    "lint": "ng lint",
    "e2e": "ENV=prod node scripts/set-env.js && ng e2e",
    "watch:front": "ENV=dev ng serve --configuration developpement",
    "watch:back": "concurrently --names \"LAMBDA,SERVICES\" \"netlify-lambda -c ./webpack.config.js serve src/functions\" \"node src/web-services/launch.js\" \"./webpack.config.js\"",
    "dev": "concurrently --names \"FRONT,BACK\" --prefix-colors \"reset.bgBlue.bold,reset.bgRed.bold\"  \"yarn:watch:front\" \"yarn:watch:back\"",
    "ci-validate": "node scripts/ci-validate.js",
    "postinstall": "node src/web-services/install.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "angular-calendar": "^0.26.6",
    "babel-cli": "^6.26.0",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.3",
    "date-fns": "^1.30.1",
    "material-icons": "^0.2.3",
    "moment": "^2.24.0",
    "netlify-identity-widget": "github:iteatime/netlify-identity-widget",
    "ng-mocks": "^7.5.0",
    "node-sass": "^4.10.0",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.120",
    "@types/netlify-identity-widget": "^1.4.1",
    "@types/node": "~8.9.4",
    "angular-mocks": "^1.7.8",
