{
  "name": "acrabadabra",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "config": "ts-node --skip-project set-env.ts",
    "build": "yarn run config && netlify-lambda -c ./webpack.config.js build src/functions && ng build --prod",
    "test": "ng test --karma-config=scripts/config/karma-phantom.conf.js",
    "e2e": "ENV=prod node scripts/set-env.js && ng e2e",
    "watch:front": "ENV=dev node scripts/set-env.js && ng serve --configuration developpement",
    "watch:back": "concurrently --names \"LAMBDA,SERVICES\" \"netlify-lambda -c ./webpack.config.js serve src/functions\" \"node src/web-services/launch.js\" \"./webpack.config.js\"",
    "dev": "concurrently --names \"FRONT,BACK\" --prefix-colors \"reset.bgBlue.bold,reset.bgRed.bold\"  \"yarn watch:front\" \"yarn watch:back\"",
    "ci-validate": "node scripts/ci-validate.js",
    "postinstall": "node src/web-services/install.js",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "netlify:dev": "netlify-lambda -c ./webpack.config.js serve src/functions",
    "tslint:ci": "ng lint --project acrabadabra",
    "tslint:fix": "ng lint --fix --project acrabadabra",
    "prettier:ci": "prettier --config ./.prettierrc --check \"src/{app,environments,assets}/**/*{.js,.json,.css,.scss}\" \"cypress/**/*.ts\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.js,.json,.css,.scss}\" \"cypress/**/*.ts\"",
    "stylelint:ci": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "stylelint --fix --cache 'src/**/*.scss'",
    "lint:ci": "yarn tslint:ci && yarn prettier:ci && yarn stylelint:ci",
    "lint:fix": "yarn tslint:fix && yarn prettier:fix && yarn stylelint:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@sweetalert2/ngx-sweetalert2": "^5.1.0",
    "@types/node": "~8.9.4",
    "angular-calendar": "^0.26.6",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "colors": "^1.4.0",
    "core-js": "^2.6.3",
    "cryptr": "^4.0.2",
    "date-fns": "^1.30.1",
    "faunadb": "^2.6.1",
    "fs": "^0.0.1-security",
    "material-icons": "^0.2.3",
    "moment": "^2.24.0",
    "mongoose": "^5.5.11",
    "netlify-identity-widget": "github:iteatime/netlify-identity-widget",
    "ng-mocks": "^7.5.0",
    "node-sass": "^4.14.1",
    "rxjs": "~6.6.3",
    "sweetalert2": "^8.12.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/core": "^10.1.2",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.120",
    "@types/netlify-identity-widget": "^1.4.1",
    "angular-mocks": "^1.7.8",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "cypress": "^5.0.0",
    "dotenv": "^8.0.0",
    "faunadb-migrate": "git+https://github.com/BrunoQuaresma/faunadb-migrate.git#master",
    "http-proxy-middleware": "^0.19.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-summary-reporter": "^1.6.0",
    "lambda-tester": "^3.5.0",
    "moment-business-days": "^1.2.0",
    "netlify-lambda": "^1.4.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "puppeteer": "^1.14.0",
    "replace-in-file": "^3.4.3",
    "stylelint": "^13.7.0",
    "stylelint-config-concentric-order": "^4.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~7.0.0",
    "tslint": "5.12.*",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.5.3",
    "webpack": "^4.30.0"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
